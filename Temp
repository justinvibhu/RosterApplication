const fs = require('fs');

// Path to your CSV file
const filePath = 'path/to/your/data.csv';

// Read the CSV file
fs.readFile(filePath, 'utf8', (err, data) => {
  if (err) {
    console.error('Error reading CSV file:', err);
    return;
  }

  // Split the CSV data into rows
  const rows = data.split('\n');
  
  // Extract the header row
  const header = rows[0].split(',');

  // Find the index of the "Age" column
  const ageIndex = header.indexOf('Age');
  
  // Select a random row index (excluding the header)
  const randomRowIndex = Math.floor(Math.random() * (rows.length - 1)) + 1;
  
  // Extract the age of the random person
  const randomPersonAge = rows[randomRowIndex].split(',')[ageIndex].trim();

  // Print the age of the random person to the console
  console.log('Age of the random person:', randomPersonAge);
});
